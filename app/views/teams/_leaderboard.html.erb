<h2>Top 10</h2>
<table>
  <thead>
    <tr>
      <th>Poř.</th>
      <th>Tým</th>
      <th>Kliků</th>
    </tr>
  </thead>

  <tbody>
    <% teams.each_with_index do |team, index| %>
      <tr>
      <td><%= index+1 %></td>
      <td><%= team.name %></td>
        <td><%= team.click_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>

